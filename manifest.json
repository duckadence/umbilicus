{
    "manifest_version": 2,
    "name": "Umbilicus",
    "description": "Focus on your work and relax with a calming lofi-music",
    "version": "0.00001",
    "icons": {
        "16": "/images/icon16.png",
        "48": "/images/icon48.png",
        "128": "/images/icon128.png"
    },
    "browser_action": {
        "default_popup": "popup.html",
        "default_title": "Umbilicus"
    },
    "background": {
        "persistent": true,
        "scripts": ["popup.js"]
    }
}
